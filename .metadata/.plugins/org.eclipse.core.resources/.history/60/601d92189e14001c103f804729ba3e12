
public class DataTypeExcersise {

	public static void main(String[] args) {

		int x = 5;
		double y = 3.21d;
		String teststring = ", test";

		System.out.println("First primitive variable: " + (x) + ", Second primitive variable " + (y)
				+ "And last but not lease, the string: " + (teststring));

	}

	public static String ReturnMethod() {
		int x = 5;
		if (x = 5) {
			return "Hello World";
		}
	}

}