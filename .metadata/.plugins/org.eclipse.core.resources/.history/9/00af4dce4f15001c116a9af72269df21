
public class calculator {

	public static int x = 10;
	public static int y = 5;

	public static void main(String[] args) {
		System.out.println(task1_add());
		System.out.println(task1_multiplication());
		System.out.println(task1_subtraction());
		System.out.println(task1_division());
	}

	private static int task1_add() {
		return x + y;
	}

	private static int task1_multiplication() {
		return x * y;
	}

	private static int task1_subtraction() {
		return x - y;
	}

	private static int task1_division() {
		if (x > y) {
			System.out.println("Division cannot be performed");
		} else {
			return (x / y);
		}
	}
}
